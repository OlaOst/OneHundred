{
  "name": "onehundred",
  "description": "An example project skeleton",
  "homepage": "http://example.org",
  "copyright": "Copyright © 2014, Ola Østtveit",
  "authors": ["Ola Østtveit"],
  "version": "0.0.1",
  "mainSourceFile": "source/app.d",
  "dependencies": {
    "derelict-sdl2": ">=1.9.0",
    "derelict-gl3": ">=1.0.10",
    "derelict-al": ">=1.0.0",
    "derelict-ogg": "~master",
    "derelict-vorbis": ">=1.0.0",
    "derelict-ft": ">=1.0.0",
    "gl3n": ">=1.0.0",
    "glamour": { "version": ">=1.0.1", "path": "../glamour" },
  },
  "importPaths" : ["../glamour"],

  "versions": ["Derelict3", "SDLImage", "gl3n"],

  "configurations": [
    {
      "name": "default",
      "targetType": "executable",
    },
    {
      "name": "test",
      "targetType": "executable",
      "targetName": "testrun",
      "sourcePaths": [
        "source/tests",
      ],
      "excludedSourceFiles": [
        "source/app.d",
      ],
      "versions": [
        "Derelict3", "SDLImage", "gl3n",
      ],
      "dependencies": {
        "derelict-sdl2": ">=1.2.8",
        "derelict-gl3": { "version": ">=1.0.10" },
      }
    },
    {
      "name": "inline",
      "targetType": "executable",
      "buildOptions": ["inline"],
    }
  ]
}
