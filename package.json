	"dependencies": {
    "derelict-sdl2": "~master",
    "derelict-gl3": "~master",
    "gl3n": "~master",
    "onehundred:artemisd": "~master",
    "onehundred:glamour": "~master",
	},
  "subPackages": [
    {
      "name": "artemisd",
      "targetType": "library",
      "sourcePaths": ["../artemisd/source"]
    },
    {
      "name": "glamour",
      "targetType": "library",
      "sourcePaths": ["../glamour/glamour"]
    }
  ],
  
  "versions": ["Derelict3", "SDLImage", "gl3n"],
  
  "configurations": [
    {
      "name": "default",
      "targetType": "executable",
    },
    {
			"name": "test",
			"targetType": "executable",
			"targetName": "testrun",
			"sourcePaths": [
				"source/tests",
			],
			"excludedSourceFiles": [
        "source/app.d",
			],
			"versions": [
				"Derelict3", "SDLImage", "gl3n",
			],
			"dependencies": {
				"derelict-sdl2": "~master",
        "derelict-gl3": "~master",
			}
		},
  ],
}
